<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="StyleK_Progress_Horizontal.Custom" parent="Widget.AppCompat.ProgressBar.Horizontal">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">5dp</item>
        <item name="android:max">100</item>
        <item name="android:progressDrawable">@drawable/drawablek_progress_rounded</item>
    </style>

    <style name="StyleK_Progress_Horizontal.Custom.Indeterminate">
        <item name="android:indeterminate">true</item>
    </style>

    <style name="StyleK_Progress" parent="Widget.AppCompat.ProgressBar">
        <item name="android:layout_width">42dp</item>
        <item name="android:layout_height">42dp</item>
    </style>

    <style name="StyleK_Progress.Indeterminate">
        <item name="android:indeterminate">true</item>
    </style>
</resources>