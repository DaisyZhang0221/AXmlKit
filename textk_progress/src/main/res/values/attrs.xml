<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="TextKProgress">
        <attr name="textKProgress_backgroundColor" format="color" />
        <attr name="textKProgress_backgroundColorStart" format="color" />
        <attr name="textKProgress_backgroundColorEnd" format="color" />
        <attr name="textKProgress_textColorDefault" format="color" />
        <attr name="textKProgress_textColorUnCover" format="color" />
        <attr name="textKProgress_textColorCover" format="color" />
        <attr name="textKProgress_borderRadius" format="dimension" />
        <attr name="textKProgress_borderWidth" format="dimension" />
        <attr name="textKProgress_borderColor" format="color" />
        <attr name="textKProgress_ballStyle" format="enum">
            <enum name="pulse" value="1" />
            <enum name="jump" value="2" />
        </attr>
        <attr name="textKProgress_ballRadius" format="dimension" />
        <attr name="textKProgress_ballSpacing" format="dimension" />
        <attr name="textKProgress_enableAnimation" format="boolean" />
        <attr name="textKProgress_minProgress" format="integer" />
        <attr name="textKProgress_maxProgress" format="integer" />
        <attr name="textKProgress_progressStyle" format="enum">
            <enum name="round" value="0" />
            <enum name="rect" value="1" />
        </attr>
    </declare-styleable>
</resources>